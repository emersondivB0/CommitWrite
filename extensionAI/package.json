{
  "name": "auto-commit",
  "displayName": "Auto Commit",
  "description": "Automatically commits changes to Git with summaries generated by OpenAI.",
  "version": "1.0.0",
  "publisher": "tu nombre de publisher",
  "engines": {
    "vscode": "^1.61.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:autoCommit"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.autoCommit",
        "title": "Auto Commit"
      }
    ],
    "configuration": {
      "type": "object",
      "title": "Auto Commit Configuration",
      "properties": {
        "autoCommit.openaiApiKey": {
          "type": "string",
          "default": "",
          "description": "API key for OpenAI."
        }
      }
    }
  },
  "dependencies": {
    "openai": "^3.0.0"
  }
}
